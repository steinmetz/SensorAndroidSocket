<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trabalho.protocolos.sensor" >

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <service android:name="jade.android.MicroRuntimeService" />

        <activity
            android:name=".SensorActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name=".AtuadorActivity"
            android:label="@string/title_activity_atuador"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".MenuInicialActivity"
            android:label="@string/title_activity_menu_inicial" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Trab7Activity"
            android:label="@string/title_activity_trab7"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name=".ControleActvity"
            android:label="@string/title_activity_controle_actvity" >
        </activity>
    </application>

</manifest>
